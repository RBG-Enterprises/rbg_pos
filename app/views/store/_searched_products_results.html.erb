
<% @products.each do |product| %>
  <% product.stocks.processed.available.where(store_front: current_store_front).each do |stock| %>
    <% if stock.balance_for_cart(current_cart) > 0 %>
      <%= render 'store/stock', stock: stock %>
    <% end %>
  <% end %>
<% end %>

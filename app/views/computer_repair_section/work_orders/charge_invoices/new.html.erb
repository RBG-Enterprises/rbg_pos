<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <div class="box box-solid">
      <div class="box-header">
        <h3 class="box-title"> Charge Invoice Details </h3>
      </div>
      <div class="box-body">
        <%= simple_form_for [@work_order, @charge_invoice], url: :computer_repair_section_work_order_charge_invoices do |f| %>
          <%= f.error_notification %>
          <%= f.input :date, as: :string, input_html: { class: "datepicker" } %>
          <%= f.input :number, as: :string %>
          <%= f.submit "Save" %>
        <% end %>
      </div>
    </div>
  </div>
</div>

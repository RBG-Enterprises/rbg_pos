<%= simple_form_for @disbursement, url: :voucher_disbursements do |f| %>
  <%= f.error_notification %>
  <%= f.input :date, as: :string, input_html: { class: "datepicker" } %>
  <%= f.input :amount %>
  <%= f.input :description %>
  <%= f.input :disburser_id, input_html: { value: current_user.id } %>
  <%= f.submit %>
<% end %>

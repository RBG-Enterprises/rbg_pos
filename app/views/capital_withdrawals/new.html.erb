<%= simple_form_for @withdraw, url: :capital_withdrawals do |f| %>
  <%= f.input :employee_id, input_html: { value: current_user.id } %>
  <%= f.input :date, input_html: { class: "datepicker" } %>
  <%= f.input :amount, input_html: { value: current_user.cash_on_hand_account_balance } %>
  <%= f.submit "Save Withdraw" %>
<% end %>

<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <div class="box box-solid">
      <div class="box-header">
        <h3 class="box-title"> Excel Upload Details </h3>
      </div>
      <div class="box-body">
        <span class="help-block"> Find excel file to upload. Please ensure file extension ends with '.xls' </span>
        <%= simple_form_for @registry, multipart: true, url: :store_front_module_purchase_order_registries do |f| %>
          <%= f.input :spreadsheet, label: false %>
          <div class="box-footer">
            <%= f.submit "Upload Purchase", class: "btn btn-primary btn-lg" %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>

wb = xlsx_package.workbook
wb.add_worksheet(name: "Stock Transfer") do |sheet|
  sheet.add_row ["Product Name", "Unit", "Barcode","Quantity", "Unit Cost", "Total Cost", "Selling Cost", "Base Measurement(UOM)", "Conversion(UOM)", "Quantity(UOM)"]
  @order.stock_transfer_order_line_items.each do |line_item|
    sheet.add_row [line_item.product_name, line_item.unit_code, line_item.bar_code, line_item.quantity, line_item.unit_cost, line_item.total_cost, line_item.product.base_measurement.price, line_item.unit_of_measurement.base_measurement.to_s.downcase, line_item.unit_of_measurement.conversion_quantity, line_item.unit_of_measurement.quantity ]
  end
end

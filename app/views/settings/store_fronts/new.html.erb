<%= simple_form_for @store_front, url: :settings_store_fronts do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :business_id, input_html: { value: current_user.business_id }%>
  <%= f.input :merchandise_inventory_account_id, collection: AccountingModule::Asset.all.map{ |a| [a.name, a.id] } %>
  <%= f.submit "Create Store Front", class: "btn btn-primary" %>
<% end %>
